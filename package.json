{
  "name": "redux-jive",
  "version": "0.0.1",
  "description": "helpers for redux wiring",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "build:dev": "./node_modules/.bin/webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:prod && npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "start": "build test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robbymurphy/redux-jive.git"
  },
  "keywords": [
    "redux",
    "react",
    "flux"
  ],
  "author": "Robert Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robbymurphy/redux-jive/issues"
  },
  "homepage": "https://github.com/robbymurphy/redux-jive#readme",
  "dependencies": {
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^21.2.1",
    "prettier-eslint": "^8.2.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.7.1"
  }
}
